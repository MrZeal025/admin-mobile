{
  "expo": {
    "name": "JuanBreath Scanner",
    "slug": "juanbreathscanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-jbscanner.png",
    "splash": {
      "image": "./assets/splash-jb.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "juanbreathdev.juanbreathscanner",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-jbscanner.png",
        "backgroundColor": "#2a749f"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
